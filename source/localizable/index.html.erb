---
title: Techies Lab
---

<%= partial "layouts/langbar" %>
<!-- background image -->
<div class="image-bg">
  <div class="bg-filter"></div>
</div>

<!-- Main -->
<section class="main text-center" id="home">
  <div class="page">
    <div class="wrapper">
      <div class="container">
        <h1 class="heading"><%= t('.header.title') %><br></h1>
        <%= image_tag "logo/logo_negative-white.png", alt: "Logo", id: "banner-img" %>
        <p id="tagline">
          <span><%= t('.header.subtitle') %></span>
        </p>
        <!-- Subscription form -->
        <form class="signup" action="//techieslab.us11.list-manage.com/subscribe/post-json?u=4e5edbd56b4da2771ce53024c&amp;id=1c0ef6f860&c=?" role="form" id="signupForm">
          <div class="input-group" style="margin: 0 auto;">
            <div class="form-group pull-left">
              <input type="email" name="EMAIL" class="form-control" placeholder="<%= t('.header.email') %>">
            </div>
            <button type="submit" class="btn btn-theme ladda-button" data-style="expand-left"><span class="ladda-label"><%= t('.header.button.notification') %></span></button>
          </div>
          <div id="mce-responses" class="clear">
            <div id="mce-error-response" class="response alert alert-danger alert-dismissible" style="display:none" role="alert">
              <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <span class="answer"></span>
            </div>
            <div id="mce-success-response" class="response alert alert-success alert-dismissible" style="display:none" role="alert">
              <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <span class="answer"></span>
            </div>
          </div>
          <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
          <div style="position: absolute; left: -5000px;"><input type="text" name="b_4e5edbd56b4da2771ce53024c_1c0ef6f860" tabindex="-1" value="">
          </div>
          <div class="clear"></div>
        </form>
        <footer class="text-center">
          <div class="social">
            <a href="https://twitter.com/TechiesLab" class="btn btn-theme"><i class="fa fa-twitter"></i></a>
            <a href="https://www.facebook.com/TechiesLabBrussels" class="btn btn-theme"><i class="fa fa-facebook"></i></a>
          </div>
        </footer>
      </div>
      <div class="arrow-down">
        <a class="scroll" href="#about" id="scroll"><i class="fa fa-angle-double-down"></i></a>
      </div>
    </div>
  </div>
</section>



<!-- Upcoming Workshops -->

<section id="workshops" class="section">
  <div class="container">
    <div class="row text-center">
      <div class="col-sm-12">
        <h3 class="section-title"><%= t('.upcoming.title') %></h3>
        <div class="section-title-border"></div>
        <%= image_tag "featured/robot_beach.jpg", alt: "Robot Drawing", width: "40%" %>
        <h3><%= t('.upcoming.program') %></h3>
      </div>
    </div>

    <div class="row text-center">
      <%= image_tag "featured/journal_bord_wm.jpg", alt: "Robot Drawing", width: "40%" %>
      <h3><%= t('.current.program') %></h3>
      <!-- <div class="row"><div class="col-sm-12"><div class="button"><%= link_to t('poll.button_fall_2020'), t('poll.link_fall_2020'), target: "_blank", class: "bb btn btn-theme"  %></div></div></div> -->

      <!-- Display image link to programme -->
      <% if I18n.locale == :fr %>
      <a href="/<%= t('.current.file') %>" target="_blank">
        <img src="/programme.png" id="program-img">
      </a>
      <% elsif I18n.locale == :nl %>
      <a href="/<%= t('.current.file') %>" target="_blank">
        <img src="/programme.png" id="program-img">
      </a>
      <% else %>
        <% link_to t('.current.file'), target: "_blank" do %>
        <%= image_tag 'programme.png', id: "program-img" %>
        <% end %>
      <% end %>
    </div>

    <div class="row">
      <div class="col-sm-12">
        <div class="row">
          <div class="col-sm-12">
            <div class="row">
              <% if data.all_events %>
              <% data.all_events.each do |event| %>
              <div class="col-sm-12 feature-box-container">
                <div class="feature-box wow fadeInDown">

                  <div class="icon"><i class="fa fa-<%= event.icon %>"></i></div>

                  <div class="description">
                    <h4 id="workshop-title"><%= t(event.title1) %></h4>
                    <div class="description-content">
                      <br>

                      <div class="text-center">
                        <%= image_tag "featured/#{t(event.image)}", alt: "coding example", width: "50%" %>
                      </div>

                      <br>
                      <p><%= t(event.description1) %></p>

                      <% if event.description2 %>
                      <p><%= t(event.description2) %></p>
                      <% end %>

                      <% if event.description3 %>
                      <p><%= t(event.description3) %></p>
                      <% end %>
                    </div>

                    <div class="description-details">

                      <div class="col-sm-4"><p><strong><%= t(event.date_drop) %></strong></div>
                      <div class="col-sm-8"><p><%= t(event.date_text) %></div>

                      <% if event.time_drop %>

                      <div class="col-sm-4"><p><strong><%= t(event.time_drop) %></strong></div>
                      <div class="col-sm-8"><p><%= t(event.time_text) %></div>
                      <% end %>

                      <div class="col-sm-4"><p><strong><%= t(event.location_drop) %></strong></div>
                      <div class="col-sm-8"><p><%= t(event.location_text) %></div>

                      <div class="col-sm-4"><p><strong><%= t(event.age_drop) %></strong></div>
                      <div class="col-sm-8"><p><%= t(event.age_text) %></div>

                      <div class="col-sm-4"><p><strong><%= t(event.language_drop) %></strong></div>
                      <div class="col-sm-8"><p><%= t(event.language_text) %></div>



                      <div class="col-sm-4"><p><strong><%= t(event.price_drop) %></strong></div>
                      <div class="col-sm-8"><p><%= t(event.price_text) %></div>

                      <% if event.price_why %>
                      <div class="col-sm-4"><p><strong><%= t(event.price_why_drop) %></strong></div>
                      <div class="col-sm-8"><p><%= t(event.price_why) %></div>
                      <% end %>


                      <% if event.equipment_text %>
                      <div class="col-sm-4"><p><strong><%= t(event.equipment_drop) %></strong></div>
                      <div class="col-sm-8"><p><%= t(event.equipment_text) %></div>
                      <% end %>
                      <div id="bb"><%= link_to t(event.button), t(event.file), target: "_blank", class: "bb btn btn-theme"  %></div>
                    </div>
                  </div>
                </div>
              </div>
              <% end %>
              <% end %>
            </div>
          </div>
        </div>


